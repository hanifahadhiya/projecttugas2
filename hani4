#pengulangan pakai for
for i in range(6):
    print("pengulangan ke - ", i)

print("\n")

listMakanan = ["jeruk", "mangga", "kiwi", "pisang", "strawberry"]
for makanan in listMakanan :
    print(makanan)
    
print("\n")

# pengulangan pakai while
angka = 1

while angka > 10 :
    angka += 1
    print("contoh penggunaan sintax while")